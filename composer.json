{
    "name": "ddetyuk/linkedin-php",
    "description": "PHP library for LinkedIn",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "league/oauth2-linkedin": "^5.1",
        "symfony/serializer": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/property-access": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "REverse\\LinkedIn\\": "lib/LinkedIn/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "REverse\\LinkedIn\\Test\\": "test/LinkedIn/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Giovanni Albero",
            "email": "giovannialbero.solinf@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy": "^1.16",
        "phpstan/phpstan": "^1.9",
        "phpspec/prophecy-phpunit": "^2.0",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
